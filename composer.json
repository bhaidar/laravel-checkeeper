{
    "name": "bhaidar/laravel-checkeeper",
    "description": "Laravel package for Checkeeper API v3 - Check mailing and PDF generation services",
    "type": "library",
    "keywords": ["laravel", "checkeeper", "check", "payment", "mailing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bilal Haidar",
            "email": "bilal@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/contracts": "^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bhaidar\\Checkeeper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bhaidar\\Checkeeper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bhaidar\\Checkeeper\\CheckkeeperServiceProvider"
            ],
            "aliases": {
                "Checkeeper": "Bhaidar\\Checkeeper\\Facades\\Checkeeper"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
